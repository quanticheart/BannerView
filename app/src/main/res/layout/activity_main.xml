<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".MainActivity">

    <com.quanticheart.core.bannerView.BannerView
            android:id="@+id/bannerView"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            app:bannerBackgroundColor="@color/colorPrimary"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:loadingBackgroundColor="@color/blue_primary_dark"
            app:loadingColor="@color/amber_primary_dark"
            app:showLoading="false"
            app:startCarouselTime="4"
            app:indicatorDotsColor="@color/Color_Palet_Flat_CARROT"
            app:indicatorDotsSelectedColor="@color/Color_Palet_Flat_SUN_FLOEWS"
            app:showPageIndicator="true"/>

</androidx.constraintlayout.widget.ConstraintLayout>